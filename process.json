{
	"apps": [{
		"name": "express-mvc",
		"script": "app.js",
		"cwd": "express-mvc/",
		"log_date_format": "YYYY-MM-DD HH:mm Z",
		"error_file": "pm2-log/error.log",
		"out_file": "pm2-log/out.log",
		"ignore_watch": ["pm2-log/"],
		"watch": false,
		"env": {
			"NODE_ENV": "development"
		},
		"env_production": {
			"NODE_ENV": "production"
		},
		"instances": "max",
		"exec_mode": "cluster"
	}, {
		"name": "mysocket",
		"script": "app.js",
		"cwd": "nodeSocket/",
		"error_file": "pm2-log/error.log",
		"out_file": "pm2-log/out.log",
		"ignore_watch": ["pm2-log/"],
		"watch": true,
		"exec_mode": "fork"
	}]
}